{
  "expo": {
    "name": "AI Drone User",
    "slug": "AIDroneUser",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/dronelogostart.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "****GOOGLE MAPS API KEY****"
      }
    },
    "android": {
      "package": "com.aidroneuser.userapp",
      "versionCode": 1,
      "config": {
        "googleMaps": { "apiKey": "****GOOGLE MAPS API KEY****" }   
     }
    }
  }
}
